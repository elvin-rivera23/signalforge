# Reserved for future config; never commit real secrets.
# Copy to .env and edit locally when needed.

# -----------------------------------------------------------------------------
# Data client config (used by app/data_client.py)
# -----------------------------------------------------------------------------
SF_PROVIDER=YAHOO_CHART         # Source: YAHOO_CHART (default, may be rate-limited)
SF_HTTP_TIMEOUT_S=4             # HTTP timeout in seconds
SF_CACHE_TTL_S=15               # Cache TTL in seconds (short for near-realtime)
SF_MAX_CACHE_SIZE=256           # Max cache entries (acts like LRU)

# -----------------------------------------------------------------------------
# Logging + runtime info
# -----------------------------------------------------------------------------
LOG_LEVEL=INFO                  # Logging level (DEBUG, INFO, WARN, ERROR)

# -----------------------------------------------------------------------------
# Model + features
# -----------------------------------------------------------------------------
MODEL_VERSION=unloaded          # Populated by release tagging or CI
FEATURESET=none                 # Feature set identifier (if multiple models)

# -----------------------------------------------------------------------------
# Build metadata (overridden by CI/CD)
# -----------------------------------------------------------------------------
GIT_COMMIT_SHA=local            # Commit SHA injected at build time
BUILD_TIME=2025-01-01T00:00:00Z # Build timestamp (default)
BUILD_TIME_OVERRIDE=            # Optional manual override
